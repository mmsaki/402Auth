    "src"
  ],
  "homepage": "https://github.com/mmsaki/402Auth",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmsaki/402Auth.git"
  },
  "bugs": {
    "url": "https://github.com/mmsaki/402Auth/issues"
  },
